	<asset>
		<texture name="dirt_tex" builtin="flat"  height="32" width="32" rgb1="1 1 1" type="cube"></texture>
		<texture name="rock0_tex" builtin="flat" height="32" width="32" rgb1="1 1 1" type="cube"></texture>
