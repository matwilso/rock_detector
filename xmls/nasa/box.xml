		<material name="right_mat" shininess="0.03" specular="0.75" texture="right_tex"></material>
		<material name="bin_mat" shininess="0.03" specular="0.75" texture="bin_tex"></material>
		<material name="dig_mat" shininess="0.03" specular="0.75" texture="dig_tex"></material>
