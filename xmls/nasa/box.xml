        <texture type="skybox"    builtin="gradient" name="skybox_tex" rgb1="1 1 1" rgb2=".6 .8 1" 
                 width="256" height="256"/>

		<material name="dirt_mat" shininess="0.03" specular="0.75" texture="dirt_tex"></material>
		<material name="rock0_mat" shininess="0.03" specular="0.75" texture="rock0_tex"></material>
